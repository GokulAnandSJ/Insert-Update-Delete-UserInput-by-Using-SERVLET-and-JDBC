<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
 
	
	<servlet>
		<servlet-name>add</servlet-name>
		<servlet-class>com.jsp.servletuser.AddUser</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>add</servlet-name>
		<url-pattern>/Add-user</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>update</servlet-name>
		<servlet-class>com.jsp.servletuser.UpdateUser</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>update</servlet-name>
		<url-pattern>/update-user</url-pattern>
	</servlet-mapping>
	
	
		<servlet>
		<servlet-name>delete</servlet-name>
		<servlet-class>com.jsp.servletuser.Delete</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>delete</servlet-name>
		<url-pattern>/delete-user</url-pattern>
	</servlet-mapping>
	
	
	
</web-app>
